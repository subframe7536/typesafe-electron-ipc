{
  "name": "typesafe-electron-ipc",
  "version": "0.6.5",
  "author": "subframe7536",
  "license": "MIT",
  "description": "typesafe ipc for electron",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./renderer": {
      "import": "./dist/renderer.mjs",
      "require": "./dist/renderer.js"
    },
    "./types": "./dist/types.d.ts"
  },
  "files": [
    "dist",
    "tsconfig.json",
    "renderer.js",
    "renderer.d.ts",
    "types.d.ts"
  ],
  "keywords": [
    "electron",
    "ipc",
    "renderer",
    "typesafe"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsup && node fix-module.js",
    "dev": "pnpm build && cd playground && pnpm dev",
    "release": "pnpm build && bumpp && npm publish",
    "lint": "eslint . --fix"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.2.9",
    "@types/node": "^20.6.3",
    "bumpp": "^9.2.0",
    "electron": "^26.2.2",
    "eslint": "^8.50.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "electron": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "object-standard-path": "^0.3.0"
  }
}
