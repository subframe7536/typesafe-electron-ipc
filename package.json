{
  "name": "typesafe-electron-ipc",
  "version": "0.5.7",
  "author": "subframe7536",
  "license": "MIT",
  "description": "typesafe ipc for electron",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./renderer": {
      "import": "./dist/esm/renderer.js",
      "require": "./dist/cjs/renderer.js"
    }
  },
  "files": [
    "dist",
    "tsconfig.json"
  ],
  "keywords": [
    "electron",
    "ipc",
    "renderer",
    "typesafe"
  ],
  "scripts": {
    "build": "node build.js",
    "dev": "pnpm build && cd playground && pnpm dev",
    "release": "pnpm build && bumpp --all && npm publish"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.1.9",
    "@types/node": "^20.3.2",
    "bumpp": "^9.1.1",
    "concurrently": "^8.2.0",
    "electron": "^24.4.0",
    "eslint": "^8.43.0",
    "typescript": "^5.1.5"
  },
  "peerDependencies": {
    "electron": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
