{
  "name": "typesafe-electron-ipc",
  "version": "0.5.1",
  "author": "subframe7536",
  "license": "MIT",
  "description": "typesafe ipc for electron",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./preload": {
      "import": "./dist/preload.mjs",
      "require": "./dist/preload.cjs"
    },
    "./renderer": {
      "import": "./dist/renderer.mjs",
      "require": "./dist/renderer.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "types": [
        "dist/types.d.ts"
      ],
      "preload": [
        "dist/preload.d.ts"
      ],
      "renderer": [
        "dist/renderer.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "electron",
    "ipc",
    "renderer",
    "typesafe"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup && cd playground && pnpm dev",
    "release": "tsup && bumpp && npm publish"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.1.9",
    "bumpp": "^9.1.1",
    "electron": "^24.4.0",
    "eslint": "^8.41.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "electron": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
